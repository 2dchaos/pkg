use uo;
use os;
use vitals;

include ":attributes:attributes";
include "include/damage";
include "include/client";
include "include/say";

program cureScript(params)
	var targ := params[1];
	var who := params[2];

	var points := AP_GetSkill(who, MAGICLORE)*0.75;
	
	//ApplyRawDamage(mobile, points);
	EraseObjProperty(targ, "#vinganca");

	DamageFLS(targ, points, DMG_FORCED, who);

	
	return 1;
endprogram
